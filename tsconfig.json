{
    "compilerOptions": {
        "moduleResolution": "node",
        "module": "ESNext",
        "allowJs": true,
        "checkJs": false,
        "jsx": "react",
        "jsxFactory": "$h",
        "jsxFragmentFactory": "Fragment",
        "target": "esnext",
        "outDir": "public/app",
        "strict": true,
        "typeRoots": ["./src/@types"]
    },
    "include": [
        "./src/*"
    ]

}
