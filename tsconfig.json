{
    "compilerOptions": {
        "moduleResolution": "bundler",
        "module": "ESNext",
        "allowJs": true,
        "checkJs": false,
        "jsx": "react",
        "jsxFactory": "$h",
        "jsxFragmentFactory": "Fragment",
        "target": "esnext",
        "strict": true,
        "baseUrl": "./",
        "skipLibCheck": true,
        "rootDir": "./src/",
        "paths": {
        }
    },
    "include": [
        "./src/*"
    ]
}
